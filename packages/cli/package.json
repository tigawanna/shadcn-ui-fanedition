{
  "name": "shadcn-fe-cli",
  "version": "0.0.3",
  "description": "Add shadcn components to your apps.",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=17.x",
    "npm": ">=8.x <9"
  },
  "license": "MIT",
  "author": {
    "name": "tigawanna",
    "url": "https://twitter.com/lettherebecode_"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tigawanna/shadcn-ui-fanedition",
    "directory": "packages/cli"
  },
  "keywords": [
    "shadcn/ui",
    "tailwind",
    "typescript",
    "node",
    "tsup",
    "fan-edition",
    "shadcn-fe-cli"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "print": "",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist && rimraf components",
    "start:dev": "cross-env COMPONENTS_REGISTRY_URL=http://localhost:3001 node dist/index.js",
    "start": "node dist/index.js",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "release": "release-it",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public",
    "ci:publish": "pnpm publish --no-git-checks --access public --tag next"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.4",
    "chalk": "5.2.0",
    "commander": "^10.0.0",
    "cosmiconfig": "^8.1.3",
    "diff": "^5.1.0",
    "execa": "^7.0.0",
    "fs-extra": "^11.1.0",
    "https-proxy-agent": "^6.2.0",
    "node-fetch": "^3.3.0",
    "ora": "^6.1.2",
    "prompts": "^2.4.2",
    "release-it": "^15.11.0",
    "ts-morph": "^18.0.0",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/diff": "^5.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/prompts": "^2.4.2",
    "rimraf": "^4.1.3",
    "tsup": "^6.6.3",
    "type-fest": "^3.8.0",
    "typescript": "^4.9.3"
  }
}
